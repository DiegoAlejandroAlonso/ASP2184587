@model ASP2184587.Models.UsuarioIndex

@{
    ViewBag.Title = "PaginadorIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card">
    <div class="card-header">
        <center><h2>PAGINADOR</h2></center>
    </div>
    <div class="card-body">
        <table class="display responsive nowrap" id="myTable" style="width:100%">            
              
                <tr>
                    <th>Nombre</th>
                    <th>Apellido</th>
                    <th>Email</th>
                </tr>

                @foreach (var usuario in Model.Usuarios)
                {
                    <tr>
                        <td>@usuario.nombre</td>
                        <td>@usuario.apellido</td>
                        <td>@usuario.email</td>
                    </tr>
                }
            
        </table>
    </div>
</div>

@{ Html.RenderPartial("Paginador", Model);}